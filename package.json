    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "prettier": "^1.15.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-gist": "^1.2.1",
    "react-router-dom": "^5.0.1",
    "storybook-react-router": "^1.0.5"
